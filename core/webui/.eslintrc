{
  "extends": ["plugin:vue/vue3-essential"],
  "ignorePatterns": ["dist", "node_modules", "assets", "public"],
  "parser": "vue-eslint-parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "parser": "@typescript-eslint/parser",
    "sourceType": "module"
  },
  "rules": {
    "no-undef": "off",
    "no-unused-vars": "off",
    "no-redeclare": "off",
    "no-console": "error",
    "vue/multi-word-component-names": "off"
  }
}
